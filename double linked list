//doubley linked list
#include<iostream>
using namespace std;
struct node{
    int data;
    node*next;
    node*prev;
    node(int value){
        data=value;
        next=NULL;
        prev=nullptr;
    }
};
void printll(node* head){
    node* temp=head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
}
int main(){
node* head =  new node(10);
head->next=new node(22);
head->next->prev=head;
head->next->next=new node(54);
head->next->next->prev=head->next;
printll(head);
}
