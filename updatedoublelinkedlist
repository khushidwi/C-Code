#include<iostream>
using namespace std;
struct Node{
    int data;
    Node*next;
    Node*pre;
    Node(int data){
        this->data=data;
        this->next=NULL;
        this->pre=NULL;
    }
};
void print(Node*head){
    Node*p=head;
    while(p!=NULL){
        cout<<p->data<<" ";
        p=p->next;
    }
    cout<<endl;
}
void insertathead(Node*&head,int value){
    Node*newnode=new Node(value);
    newnode->next=head;
    head->pre=newnode;
    head=newnode;
}
void insertattail(Node* &tail,int value){
    Node*newnode=new Node(value);
    tail->next=newnode;
    newnode->pre=tail;
    tail=newnode;
}
void insertatmiddle(Node* &head,int pos,int value){
    Node*p=head;
    Node*q=NULL;
    Node*pre=nullptr;
    for(int i=0;i<=pos;i++){
        p=q;
        p=p->next;
    }
    Node* newnode=new Node(value); 
    newnode->next=p;
    newnode->pre= q;
    q->next=newnode;
    p->pre=newnode;
}
int main(){
    Node*node1=new Node(10);
    Node*head=node1;
    Node*tail=node1;
    print(head);
    insertathead(head,78);
    print(head);
    insertathead(head,90);
    print(head);
    insertattail(tail,12);
    print(head);
    insertattail(tail,32);
    print(head);
    insertatmiddle(head,2,55);
    print(head);
}
