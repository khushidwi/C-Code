#include<iostream>
using namespace std;
struct Node{
    int data;
    Node*next;
    Node(int data){
        this->data=data;
        this->next=NULL;
    }
};

void print(Node*head){
    Node*p=head;
    while(p!=NULL){
        cout<<p->data<<" ";
        p=p->next;
    }
    cout<<endl;
}
int main(){
    Node*head=new Node(12);
    head->next=new Node(22);
    head->next->next=new Node(76);
    head->next->next->next=new Node(32);
    head->next->next->next->next=new Node(88);
    head->next->next->next->next->next=new Node(99);
    head->next->next->next->next->next->next=head->next->next;
    
    
    Node*slow=head;
    Node*fast=head;
    bool flag=false;
    while(fast->next!=NULL&&fast->next->next!=NULL){
        slow=slow->next;
        fast=fast->next->next;
        if(slow==fast){
            flag=true;
            break;
        }
    }
    if(flag){
        cout<<"yes"<<endl;
    }
    else{
        cout<<"no"<<endl;
    }
}
//detecting in which position loop is present 
    Node*slow1=head;
    Node*fast1=head;
    while(fast1->next!=NULL&&fast1->next->next!=NULL){
        slow1=slow1->next;
        fast1=fast1->next->next;
        if(slow1==fast1){
            slow1=head;
            while(slow1!=fast1){
                fast1=fast1->next;
                slow1=slow1->next;
            }
            cout<<slow1->data;
            break;
        }
    }
