#include<iostream>
using namespace std;
class stack{
    public:
    int arr[100];
    int top;
    stack(){
        top=-1;
    }

    void push(int value){
        if(top==100){
            cout<<"stack overflow";
        }
        else{
            top++;
            arr[top]=value;
        }
    }
    void pop(){
        if(top==-1){
            cout<<"stack is underflow";
        }
        else{
            top--;
        }
    }
    bool isempty(){
        if(top==-1)
        {
            return true;
        }
        else
        {
            return false;
        }
        //return top==-1;
    }
    int peek(){
        if(top==-1){
            return -1;
        }
        return arr[top];
    }
    void traversal(){
        for(int i=top;i>=0;i--){
            cout<<arr[i]<<" ";
        }
        cout<<endl;
    }
};
int main(){
